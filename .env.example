# Example Environment Configuration
# Copy this file to .env.development, .env.staging, or .env.production and fill in the values

# Environment (development, staging, or production)
EXPO_PUBLIC_ENV=development

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration
EXPO_PUBLIC_API_URL=your_api_url

# Analytics and Error Tracking
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn
EXPO_PUBLIC_POSTHOG_API_KEY=your_posthog_api_key
EXPO_PUBLIC_POSTHOG_HOST=your_posthog_host

# Add other environment variables as needed
# EXPO_PUBLIC_FEATURE_FLAG_NEW_UI=true 
